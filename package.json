  "homepage": "https://github.com/elierotenberg/prisma-directus#readme",
  "license": "UNLICENSED",
  "name": "prisma-directus",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/elierotenberg/prisma-directus.git"
  },
  "scripts": {
    "ws:build": "lerna run build",
    "ws:clean": "lerna clean && rm -rf node_modules && rm package-lock.json",
    "ws:install": "npm i --legacy-peer-deps --prefer-dedupe",
    "ws:ncu": "ncu -ws --root -u -t minor -i",
    "ws:ncu:latest": "ncu -ws --root -u -t latest -i",
    "ws:publish": "lerna publish"
  },
  "type": "commonjs",
  "workspaces": [
    "./packages/*"
  ],
  "devDependencies": {
    "lerna": "^8.1.2",
    "npm-check-updates": "^16.14.20"
  }
}